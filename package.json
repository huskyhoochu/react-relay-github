{
  "name": "react-relay-github",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production webpack --progress --color && workbox generateSW workbox-config.js",
    "dev": "rm -rf dist && NODE_ENV=development NODE_OPTIONS='--openssl-legacy-provider' webpack serve",
    "relay": "relay-compiler --src ./src --schema ./data/schema.docs.graphql --language typescript",
    "start": "pm2 serve ./dist 3030 --spa --no-daemon",
    "prettier": "prettier --write",
    "eslint": "eslint --fix"
  },
  "lint-staged": {
    "./src/**/*.(ts|tsx|scss)": [
      "pnpm prettier"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/plugin-transform-async-to-generator": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@types/node": "^14.18.63",
    "@types/numeral": "^2.0.5",
    "@types/react": "^17.0.87",
    "@types/react-dom": "^17.0.26",
    "@types/react-helmet": "^6.1.11",
    "@types/react-relay": "^7.0.31",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.4.1",
    "babel-plugin-relay": "^10.1.3",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^6.0.4",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.37.5",
    "file-loader": "^6.2.0",
    "graphql": "^15.10.1",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.6.2",
    "pm2": "^4.5.6",
    "postcss": "^8.5.5",
    "postcss-loader": "^8.1.1",
    "prettier": "^2.8.8",
    "relay-compiler": "^10.1.3",
    "relay-compiler-language-typescript": "^13.0.10",
    "relay-config": "^10.1.3",
    "sass": "^1.89.2",
    "sass-loader": "^10.5.2",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "typescript": "^4.9.5",
    "uglify-js": "^3.19.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^3.11.3",
    "workbox-cli": "^6.6.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.19",
    "animate.css": "^4.1.1",
    "date-fns": "^2.30.0",
    "dotenv": "^8.6.0",
    "isomorphic-fetch": "^3.0.0",
    "numeral": "^2.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-relay": "^10.1.3",
    "react-router-dom": "^5.3.4",
    "relay-runtime": "^10.1.3",
    "tailwindcss": "^2.2.19"
  }
}
